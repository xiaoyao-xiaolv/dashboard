{
    "dataBinding": {
        "dataRoles": [
            {
                "name": "status",
                "displayNameKey": "运行状态",
                "kind": "grouping"
            },
            {
                "name": "startTime",
                "displayNameKey": "开始时间",
                "kind": "grouping",
                "_noHierarchy": true,
                "acceptedTypes": ["datetime"]
            },
            {
                "name": "endTime",
                "displayNameKey": "结束时间",
                "kind": "grouping",
                "_noHierarchy": true,
                "acceptedTypes": ["datetime"]
            },
            {
                "name": "workTime",
                "displayNameKey": "上班时间",
                "kind": "grouping",
                "_noHierarchy": true,
                "acceptedTypes": ["datetime"]
            },
            {
                "name": "offTime",
                "displayNameKey": "下班时间",
                "kind": "grouping",
                "_noHierarchy": true,
                "acceptedTypes": ["datetime"]
            },
            {
                "name": "categories",
                "displayNameKey": "分类",
                "kind": "grouping"
            }
        ],
        "dataViewMappings": [
            {
                "plain": {
                    "required": [
                        {
                            "status": {
                                "min": 1
                            },
                            "startTime": {
                                "min": 1
                            },
                            "endTime": {
                                "min": 1
                            },
                            "workTime": {
                                "min": 1
                            },
                            "offTime": {
                                "min": 1
                            }
                        }
                    ]
                }
            }
        ],
        "conditions": [
            {
                "status": {
                    "max": 1
                },
                "startTime": {
                    "max": 1
                },
                "endTime": {
                    "max": 1
                },
                "workTime": {
                    "max": 1
                },
                "offTime": {
                    "max": 1
                },
                "category": {
                    "max": 1
                }
            }
        ]
    },
    "options": {
        "visual": [
            {
                "displayName": "图表样式",
                "properties": [
                    {
                        "name": "statusColors",
                        "type": "Palette",
                        "displayName": "状态颜色"
                    },
                    {
                        "name": "statusBgColors",
                        "type": "Color",
                        "displayName": "状态背景颜色",
                        "defaultValue": "rgba(255, 255, 255, 0.2)"
                    },
                    {
                        "name": "height",
                        "type": "Float",
                        "displayName": "柱子高度",
                        "defaultValue": 50.0,
                        "min": 10.0
                    }
                ]
            },
            {
                "displayName": "分类轴",
                "properties": [
                    {
                        "name": "showAxisLabel",
                        "type": "Boolean",
                        "displayName": "显示分类轴标签",
                        "defaultValue": true
                    },
                    {
                        "name": "yAxisTextStyle",
                        "type": "TextStyle",
                        "displayName": "分类轴字体设置",
                        "inheritFrom": "dashboard.textStyle",
                        "defaultValue": {
                            "fontSize": "1.2x"
                        }
                    }
                ]
            },
            {
                "displayName": "时间轴",
                "properties": [
                    {
                        "name": "startCustom",
                        "type": "Boolean",
                        "displayName": "开启自定义时间格式",
                        "defaultValue": false
                    },
                    {
                        "name": "customTimeFormat",
                        "type": "Text",
                        "displayName": "自定义时间格式",
                        "defaultValue": "{HH}:{mm}"
                    },
                    {
                        "name": "timeFormat",
                        "type": "Enum",
                        "displayName": "时间格式",
                        "items": [
                            {
                                "value": "{yyyy}-{MM}-{dd} {hh}:{mm}:{ss}",
                                "displayNameKey": "精确日期"
                            },
                            {
                                "value": "{HH}:{mm}",
                                "displayNameKey": "时分"
                            },
                            {
                                "value": "{HH}:{mm}:{ss}",
                                "displayNameKey": "时分秒"
                            },
                            {
                                "value": "{yyyy}-{MM}-{dd}",
                                "displayNameKey": "年-月-日"
                            },
                            {
                                "value": "{yyyy}-{MM}",
                                "displayNameKey": "年-月"
                            },
                            {
                                "value": "{MM}-{dd}",
                                "displayNameKey": "月-日"
                            },
                            {
                                "value": "{eeee}",
                                "displayNameKey": "星期"
                            },
                            {
                                "value": "{yyyy}",
                                "displayNameKey": "年份"
                            },
                            {
                                "value": "{MMM}",
                                "displayNameKey": "月份"
                            },
                            {
                                "value": "{H}",
                                "displayNameKey": "时"
                            },
                            {
                                "value": "{m}",
                                "displayNameKey": "分"
                            },
                            {
                                "value": "{s}",
                                "displayNameKey": "秒"
                            }
                        ],
                        "defaultValue": "{HH}:{mm}"
                    },
                    {
                        "name": "aAxisTextStyle",
                        "type": "TextStyle",
                        "displayName": "时间轴字体设置",
                        "inheritFrom": "dashboard.textStyle",
                        "defaultValue": {
                            "fontSize": "1.2x"
                        }
                    }
                ]
            },
            {
                "displayName": "图例",
                "properties": [
                    {
                        "name": "showLegend",
                        "type": "Boolean",
                        "displayName": "显示图例",
                        "defaultValue": true
                    },
                    {
                        "name": "legendIcon",
                        "type": "Enum",
                        "displayName": "图标",
                        "items": [
                            {
                                "value": "circle",
                                "displayNameKey": "圆形"
                            },
                            {
                                "value": "rect",
                                "displayNameKey": "方形"
                            },
                            {
                                "value": "roundRect",
                                "displayNameKey": "圆角方形"
                            },
                            {
                                "value": "triangle",
                                "displayNameKey": "三角形"
                            },
                            {
                                "value": "diamond",
                                "displayNameKey": "菱形"
                            }
                        ],
                        "defaultValue": "roundRect"
                    },
                    {
                        "name": "legendOrient",
                        "type": "Enum",
                        "displayName": "图例布局方向",
                        "items": [
                            {
                                "value": "horizontal",
                                "displayNameKey": "水平方向"
                            },
                            {
                                "value": "vertical",
                                "displayNameKey": "垂直方向"
                            }
                        ],
                        "defaultValue": "horizontal"
                    },
                    {
                        "name": "legendHorizontalPosition",
                        "type": "Enum",
                        "displayName": "水平位置",
                        "items": [
                            {
                                "value": "left",
                                "displayNameKey": "居左"
                            },
                            {
                                "value": "center",
                                "displayNameKey": "居中"
                            },
                            {
                                "value": "right",
                                "displayNameKey": "居右"
                            }
                        ],
                        "defaultValue": "center"
                    },
                    {
                        "name": "legendVerticalPosition",
                        "type": "Enum",
                        "displayName": "垂直位置",
                        "items": [
                            {
                                "value": "top",
                                "displayNameKey": "顶部"
                            },
                            {
                                "value": "middle",
                                "displayNameKey": "居中"
                            },
                            {
                                "value": "bottom",
                                "displayNameKey": "底部"
                            }
                        ],
                        "defaultValue": "bottom"
                    },
                    {
                        "name": "legendTextStyle",
                        "type": "TextStyle",
                        "displayName": "字体设置",
                        "inheritFrom": "dashboard.textStyle",
                        "defaultValue": {
                            "fontSize": "1.2x"
                        }
                    }
                ]
            },
            {
                "type": "title"
            },
            {
                "type": "interaction"
            },
            {
                "type": "appearance"
            }
        ]
    },
    "actions": [
        {
            "type": "focus"
        },
        {
            "type": "annotation"
        }
    ],
    "analysis": []
}
