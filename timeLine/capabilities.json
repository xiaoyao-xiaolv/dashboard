{
    "dataBinding": {
        "dataRoles": [
            {
                "name": "name",
                "displayNameKey": "节点名称",
                "kind": "grouping"
            },
            {
                "name": "value",
                "displayNameKey": "节点状态",
                "kind": "value"
            },
            {
                "name": "image",
                "displayNameKey": "节点图片",
                "kind": "grouping"
            },
            {
                "name": "describe",
                "displayNameKey": "节点详情",
                "kind": "grouping"
            }
        ],
        "dataViewMappings": [
            {
                "plain": {
                    "required": [
                        {
                            "name": {
                                "min": 1
                            },
                            "value": {
                                "min": 0
                            },
                            "image": {
                                "min": 0
                            },
                            "describe": {
                                "min": 0
                            }
                        }
                    ]
                }
            }
        ],
        "conditions": [
            {
                "name": {
                    "max": 1
                },
                "value": {
                    "max": 1
                },
                "image": {
                    "max": 1
                }
            }
        ]
    },
    "options": {
        "visual": [ {
            "displayName": "图表样式",
            "properties": [
                {
                    "name": "timeLineDirection",
                    "type": "Enum",
                    "displayName": "轴方向",
                    "items": [
                        {
                            "value": "horizontal",
                            "displayNameKey": "横向"
                        },
                        {
                            "value": "vertical",
                            "displayNameKey": "纵向"
                        },{
                            "value": "auto",
                            "displayNameKey": "自适应"
                        }
                    ],
                    "defaultValue": "horizontal"
                },
                {
                    "name": "horizontalItemsLayout",
                    "type": "Enum",
                    "displayName": "横向标签方向",
                    "items": [
                        {
                            "value": "top",
                            "displayNameKey": "居上"
                        },
                        {
                            "value": "bottom",
                            "displayNameKey": "居下"
                        },{
                            "value": "auto",
                            "displayNameKey": "上下"
                        }
                    ],
                    "defaultValue": "auto"
                },
                {
                    "name": "verticalItemsLayout",
                    "type": "Enum",
                    "displayName": "纵向标签方向",
                    "items": [
                        {
                            "value": "right",
                            "displayNameKey": "居左"
                        },
                        {
                            "value": "left",
                            "displayNameKey": "居右"
                        },{
                            "value": "auto",
                            "displayNameKey": "左右"
                        }
                    ],
                    "defaultValue": "auto"
                },
                
                {
                    "name": "showLabel",
                    "type": "Enum",
                    "displayName": "显示方式",
                    "items": [
                        {
                            "value": "none",
                            "displayNameKey": "无"
                        },
                        {
                            "value": "text",
                            "displayNameKey": "标题"
                        },
                        {
                            "value": "title",
                            "displayNameKey": "标题标签"
                        },
                        {
                            "value": "content",
                            "displayNameKey": "标题和内容标签"
                        }
                    ],
                    "defaultValue": "content"
                },
                {
                    "name": "labelTitleAlign",
                    "type": "Enum",
                    "displayName": "标题对齐",
                    "items": [
                        {
                            "value": "left",
                            "displayNameKey": "居左"
                        },
                        {
                            "value": "center",
                            "displayNameKey": "居中"
                        },{
                            "value": "right",
                            "displayNameKey": "居右"
                        }
                    ],
                    "defaultValue": "center"
                },
                {
                    "name": "labelTitleContentAlign",
                    "type": "Enum",
                    "displayName": "标题对齐",
                    "items": [
                        {
                            "value": "left",
                            "displayNameKey": "居左"
                        },
                        {
                            "value": "center",
                            "displayNameKey": "居中"
                        },{
                            "value": "right",
                            "displayNameKey": "居右"
                        }
                    ],
                    "defaultValue": "left"
                },
                {
                    "name": "labelContentAlign",
                    "type": "Enum",
                    "displayName": "内容对齐",
                    "items": [
                        {
                            "value": "left",
                            "displayNameKey": "居左"
                        },
                        {
                            "value": "center",
                            "displayNameKey": "居中"
                        },{
                            "value": "right",
                            "displayNameKey": "居右"
                        }
                    ],
                    "defaultValue": "left"
                },
                {
                    "name": "labelBg",
                    "type": "Color",
                    "displayName": "标签背景",
                    "defaultValue": "#fff"
                },
                {
                    "name": "labelTextStyle",
                    "type": "TextStyle",
                        "displayName": "标题设置",
                        "inheritFrom": "dashboard.textStyle",
                        "defaultValue": {
                            "fontSize": "1.2X",
                            "color": "#CCC"
                        }
                },
                {
                    "name": "labelDescribeStyle",
                    "type": "TextStyle",
                        "displayName": "内容设置",
                        "inheritFrom": "dashboard.textStyle",
                        "defaultValue": {
                            "fontSize": "1.0X",
                            "color": "#CCC"
                        }
                }
            ]
        }, 
        {
            "displayName": "连接线节点样式",
            "properties": [
                {
                    "name": "timeLineBg",
                    "type": "Color",
                    "displayName": "连接线颜色",
                    "defaultValue": "#fff"
                },
                {
                    "name": "timeLineSize",
                    "type": "Integer",
                    "displayName": "连接线粗细",
                    "defaultValue": 4
                },
                {
                    "name": "visibleItems",
                    "type": "Enum",
                    "displayName": "节点个数",
                    "items": [
                        {
                            "value": "default",
                            "displayNameKey": "均匀分布"
                        },
                        {
                            "value": "custom",
                            "displayNameKey": "自定义"
                        }
                    ],
                    "defaultValue": "default"
                },
                {
                    "name": "customVisibleItems",
                    "type": "Integer",
                    "displayName": "调节个数",
                    "defaultValue": 1,
                    "min": 1
                },
                {
                    "name": "timeLinePointSize",
                    "type": "Integer",
                    "displayName": "节点大小",
                    "defaultValue": 20
                },
                {
                    "name": "timeLinePointBorder",
                    "type": "Integer",
                    "displayName": "节点边框线",
                    "defaultValue": 4
                },
                {
                    "name": "timeLinePointColor",
                    "type": "Color",
                    "displayName": "节点颜色",
                    "defaultValue": "#fff"
                },
                {
                    "name": "timeLinePointBg",
                    "type": "Color",
                    "displayName": "节点边框色",
                    "defaultValue": "#DDD"
                }
            ]
        },
        {
            "displayName": "条件样式",
            "properties": [
                {
                    "name": "useToPoint",
                    "type": "Boolean",
                    "displayName": "作用到节点",
                    "defaultValue": true
                },
                {
                    "name": "useToLabel",
                    "type": "Boolean",
                    "displayName": "作用到标签",
                    "defaultValue": true
                },
                {
                    "name": "timeLineCollection",
                    "displayName": "时间条件列表",
                    "type": "Collection",
                    "itemShape": [
                        {
                            "name": "formatValue",
                            "displayName": "条件数值",
                            "type": "Text"
                        },
                        {
                            "name": "formatColor",
                            "displayName": "条件颜色",
                            "type": "Color"
                        }, 
                        {
                            "name": "formatBorderColor",
                            "displayName": "条件边框颜色",
                            "type": "Color"
                        }, 
                        {
                            "name": "formatImage",
                            "displayName": "条件图片",
                            "type": "Image"
                        }
                    ],
                    "defaultValue": []
                }
            ]
        }
    ]
    }, 
    "actions": [],
    "analysis": []
}