{
  "compilerOptions": {
    "noImplicitAny": false,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "types": ["react", "react-dom"],
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "strictNullChecks": false,
    "lib": [
      "es2015",
      "dom"
    ]
  }
}