                        "displayName": "图标样式",
                        "items": [{
                            "value": "qp_1",
                            "displayName": "蓝色"
                        }, {
                            "value": "qp_2",
                            "displayName": "黑色"
                        },{
                            "value": "qp_3",
                            "displayName": "紫色"
