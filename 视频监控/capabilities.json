                            "value": "qp_7",
                            "displayName": "红色"
                        },{
                            "value": "qp_8",
                            "displayName": "白色"
                        }],
                        "defaultValue": "qp_5"
                    }
                ]
            },

            {
                "displayName": "弹出设置",
                "properties": [
                    {
                        "name": "title_div",
                        "displayName": "标签样式",
                        "type": "TextStyle",
                        "inheritFrom": "dashboard.textStyle"
                    },
                    {
                        "name": "div_loc",
                        "type": "Enum",
                        "displayName": "对齐方式",
                        "items": [{
                            "value": "left",
                            "displayName": "左对齐"
                        }, {
                            "value": "center",
                            "displayName": "居中"
                        }],
                        "defaultValue": "left"
                    },
                    {
                        "name": "div_height",
                        "displayName": "标签高度",
                        "type": "Integer",
                        "min": 1,
                        "defaultValue": 25
                    },
                    {
                        "name": "div_color",
                        "type": "Color",
                        "displayName": "标签背景",
                        "defaultValue": "#0062b1"
                    },
                    {
                        "name": "div_width",
                        "displayName": "图标大小",
                        "type": "Integer",
                        "min": 1,
                        "defaultValue": 14
                    },
                    {
                        "name": "div_style",
                        "type": "Enum",
                        "displayName": "图标样式",
                        "items": [{
                            "value": "gb_1",
                            "displayName": "蓝色"
                        }, {
                            "value": "gb_2",
                            "displayName": "黑色"
                        },{
                            "value": "gb_3",
                            "displayName": "紫色"
                        },{
                            "value": "gb_4",
                            "displayName": "绿色"
                        },{
                            "value": "gb_5",
                            "displayName": "浅灰色"
                        },{
                            "value": "gb_6",
                            "displayName": "深灰色"
                        },{
                            "value": "gb_7",
                            "displayName": "红色"
                        },{
                            "value": "gb_8",
                            "displayName": "白色"
                        }],
                        "defaultValue": "gb_5"
                    },
                    {
                        "name": "v_radius",
                        "type": "Integer",
                        "min": 0,
                        "max": 90,
                        "displayName": "圆角",
                        "defaultValue": 3
                    },
                    {
                        "name": "v_border",
                        "type": "Integer",
                        "min": 0,
                        "max": 100,
                        "displayName": "边框宽度",
                        "defaultValue": 3
                    },
                    {
                        "name": "v_solid",
                        "type": "Color",
                        "displayName": "边框颜色",
                        "defaultValue": "#0062b1"
                    }
                ]
            },
            {
                "type": "title"
            },
            {
                "type": "interaction"
            }
        ]
    },
    "actions": [{
        "type": "focus"
    }, {
        "type": "filter"
