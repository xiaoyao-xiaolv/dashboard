{
    "dataBinding": {
        "dataRoles": [
            {
                "name": "ActualValue",
                "displayNameKey": "实际值",
                "kind": "value"
            },
            {
                "name": "ContrastValue",
                "displayNameKey": "对比值",
                "kind": "value"
            }
        ],
        "dataViewMappings": [
            {
                "plain": {
                    "required": [
                        {
                            "ActualValue": {
                                "min": 1
                            }
                        },
                        {
                            "ContrastValue": {
                                "min": 1
                            }
                        }
                    ]
                }
            }
        ],
        "conditions": [
            {
                "ActualValue": {
                    "max": 1
                },
                "ContrastValue": {
                    "max": 1
                }
            }
        ]
    },
    "options": {
        "visual": [
            {
                "displayName": "图表样式",
                "properties": [
                    {
                        "displayNameKey": "水球形状",
                        "name": "shape",
                        "type": "Enum",
                        "items": [
                            {
                                "value": "rect",
                                "displayNameKey": "rect"
                            },
                            {
                                "value": "roundRect",
                                "displayNameKey": "roundRect"
                            },
                            {
                                "value": "circle",
                                "displayNameKey": "circle"
                            },
                            {
                                "value": "diamond",
                                "displayNameKey": "diamond"
                            }
