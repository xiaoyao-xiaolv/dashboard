                    },
                    {
                        "name": "outlineborderWidth",
                        "type": "Integer",
                        "displayName": "边框线宽",
                        "defaultValue": "4"
                    },
                    {
                        "name": "outlineborderColor",
                        "type": "Color",
                        "displayName": "边框颜色",
                        "defaultValue": "#0ff"
