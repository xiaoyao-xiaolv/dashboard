                        "itemShape": [
                            {
                                "name": "sectionMin",
                                "type": "Integer",
                                "min": 0,
                                "displayName": "大于"
                            },
                            {
                                "name": "sectionMax",
                                "type": "Integer",
                                "max": 1000,
                                "displayName": "小于等于"
                            }
                        ]
                    }
                ]
            },
            {
                "displayName": "内边框样式",
