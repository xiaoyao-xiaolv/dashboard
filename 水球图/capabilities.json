                    },
                    {
                        "name": "borderColor",
                        "type": "Color",
                        "displayName": "边框颜色",
                        "defaultValue": "#1daaeb"
                    }
                ]
            },
            {
                "displayName": "外边框样式",
                "properties": [
                    {
                        "name": "showoutline",
                        "type": "Boolean",
